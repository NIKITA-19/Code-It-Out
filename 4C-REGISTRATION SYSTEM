#include <iostream>
#include <string>
#include <vector>
#include <bits/stdc++.h>
using namespace std;
 
int main() {
	ios_base::sync_with_stdio(false);
	cin.tie(NULL);
	int n,i;
	string s;
	map<string,int>m;
	cin>>n;
	for(i=0;i<n;i++)
	{
		cin>>s;
		if(m[s]==0)
		{
			cout<<"OK"<<endl;
			m[s]++;
		}
		else
		{
			cout<<s<<m[s]<<endl;
			m[s]++;
		}
	}
	return 0;
}
